{
  "$schema": "./appsettings.schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning",
      "Paycheck4.Core.Usb": "Warning"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": false,
        "SingleLine": true
      }
    }
  },
  "Protocol": {
    "StatusReportingInterval": 2000,
    "PrintStartDelayInterval": 3000,
    "ValidationDelayInterval": 18000,
    "BusyStateChangeInterval": 20000,
    "TOFStateChangeInterval": 4000,
    "PaperInChuteSetInterval": 3000,
    "PaperInChuteClearInterval": 10000
  }
}